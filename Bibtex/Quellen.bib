@ARTICLE{Kocher1996,
  author = {Kocher, Paul C.},
  title = {Timing Attacks on Implementations of Diffie-Hellman, RSA, DSS, and
	Other Systems},
  year = {1996},
  series = {CRYPTO '96},
  pages = {104-113},
  url = {http://www.cryptography.com/public/pdf/TimingAttacks.pdf},
  abstract = {By carefully measuring the amount of time required tm perform private
	key operalions, attackers may be able to find fixed Diffie-Hellman
	exponents, factor RSA keys, and break other cryptosystems. Against,
	a valnerable system, the attack is computationally inexpensive and
	often requires only known ciphertext. Actual systems are potentially
	at risk, including cryptographic tokens, network-based cryptosystems,
	and other applications where attackers can make reasonably accurate
	timing measurements. Techniques for preventing the attack for RSA
	and Diffie-Hellman are presented. Some cryptosystems will need to
	be revised to protect against the attack, and new protocols and algorithms
	may need to incorporate measures to prevenl timing attacks.},
  address = {Berlin},
  citeulike-article-id = {8265141},
  citeulike-linkout-0 = {http://portal.acm.org/citation.cfm?id=706156},
  isbn = {3-540-61512-1},
  journal = {Proceedings of the 16th Annual International Cryptology Conference
	on Advances in Cryptology},
  keywords = {literatur},
  posted-at = {2010-11-17 08:15:20},
  priority = {2},
  publisher = {Springer-Verl.}
}
